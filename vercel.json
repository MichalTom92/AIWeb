{
  "version": 2,
  "builds": [
    { "src": "back/server.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "/back/server.js"
    },
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}